graph {
  hq -- xt;
  OU -- xt;
  br -- HP;
  WD -- xt;
  end -- br;
  start -- OU;
  hq -- br;
  MH -- hq;
  MH -- start;
  xt -- br;
  end -- WD;
  hq -- start;
  MH -- br;
  qw -- OU;
  hm -- WD;
  br -- WD;
  OU -- hq;
  xt -- MH;
  qw -- MH;
  WD -- qw;
  end -- qw;
  qw -- xt;
}

